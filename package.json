{
  "name": "@theorhd/deepmemorymcp",
  "version": "1.2.1",
  "description": "Model Context Protocol server for long-term memory management with SQLite storage",
  "type": "module",
  "main": "dist/index.js",
  "bin": "bin/cli.js",
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rimraf dist",
    "publish": "npm run clean && npm run build && npm publish --access public"
  },
  "keywords": [
    "mcp",
    "memory",
    "ai",
    "storage",
    "sqlite",
    "long-term"
  ],
  "author": "Theorhd",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/better-sqlite3": "^7.6.8",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^9.2.2",
    "cryptojs-secure": "^0.1.2",
    "mysql2": "^3.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
